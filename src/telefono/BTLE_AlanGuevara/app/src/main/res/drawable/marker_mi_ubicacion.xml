<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Círculo exterior (borde) -->
    <item>
        <shape android:shape="oval">
            <size
                android:width="24dp"
                android:height="24dp" />
            <solid android:color="#005965" />   <!-- borde oscuro -->
        </shape>
    </item>

    <!-- Círculo interior (relleno) -->
    <item
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#1F8A93" />  <!-- color interior -->
        </shape>
    </item>

</layer-list>
